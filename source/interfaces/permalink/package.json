{
  "name": "@jamtastic/directus-extension-permalink",
  "version": "0.5.1",
  "description": "Permalink interface to enter URL slugs for Directus",
  "license": "ISC",
  "author": "Jamtastic! (https://jamtastic.dev/)",
  "contributors": [
    "Benny Michaels <foss@bnry.be> (https://bnry.be/)"
  ],
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-interface",
    "slug",
    "permalink"
  ],
  "homepage": "https://github.com/jamtastic/directus/source/interfaces/permalink#readme",
  "bugs": {
    "url": "https://github.com/jamtastic/directus/issues"
  },
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "interface",
    "path": "dist/permalink.js",
    "source": "source/permalink.ts",
    "host": "^9.20.4"
  },
  "scripts": {
    "prebuild": "npm run lint",
    "build": "directus-extension build",
    "fix": "npm run fix:code && npm run fix:code && npm run fix:style",
    "fix:code": "eslint **/*.{vue,ts} --fix",
    "fix:format": "prettier --write **/*.{vue,ts}",
    "fix:style": "stylelint **/*.{vue,css,scss,sass}",
    "dev": "directus-extension build -w --no-minify",
    "lint": "npm run lint:code && npm run lint:code && npm run lint:style",
    "lint:code": "eslint **/*.{vue,ts}",
    "lint:format": "prettier **/*.{vue,ts}",
    "lint:style": "stylelint **/*.{vue,css,scss,sass}"
  },
  "eslintConfig": {
    "extends": "@jamtastic/eslint-config-directus"
  },
  "prettier": "@jamtastic/directus-standardish/prettier",
  "stylelint": {
    "extends": "@jamtastic/directus-standardish/stylelint"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "9.21.0",
    "@jamtastic/directus-standardish": "^0.9.0",
    "typescript": "^4.9.3",
    "vue": "^3.2.45"
  },
  "dependencies": {
    "@directus/shared": "^9.21.0",
    "directus-extension-display-link": "^1.6.1"
  }
}
