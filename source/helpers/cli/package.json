{
  "name": "@jamtastic/bunny",
  "version": "0.0.0",
  "description": "CLI to assist development in @jamtastic/directus",
  "author": "Jamtastic! (https://jamtastic.dev/)",
  "homepage": "https://github.com/jamtastic/directus",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jamtastic/directus.git",
    "directory": "source/helpers/cli"
  },
  "bugs": {
    "url": "https://github.com/jamtastic/directus/issues"
  },
  "bin": {
    "bunny": "./bin/run"
  },
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "eslintConfig": {
    "extends": "@jamtastic/eslint-config-directus"
  },
  "prettier": "@jamtastic/directus-standardish/prettier",
  "dependencies": {
    "@oclif/core": "^1.19.1",
    "@oclif/plugin-help": "^5"
  },
  "devDependencies": {
    "@jamtastic/directus-standardish": "0.9.0",
    "@oclif/test": "^2.2.10",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.3",
    "chai": "^4",
    "eslint": "^7.32.0",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "oclif": {
    "bin": "bunny",
    "dirname": "cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "new": {
        "description": "Create new extensions for Directus"
      },
      "qa": {
        "description": "Ensure all extensions are high quality"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "format": "prettier **/*.ts --write",
    "lint": "eslint . --ext .ts",
    "prelint": "npm run format",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
